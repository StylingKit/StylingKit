<test>
	<title>Substring matching attribute value selector on end with declared namespace</title>
	<style type="text/css">@namespace a url(http://www.example.org/a);
@namespace b url(http://www.example.org/b);
@namespace html url(http://www.w3.org/1999/xhtml);
*|p, *|q, *|r, *|s { display : block ; margin-bottom : 1em }
*|p, *|r { background-color : red }
*|*[a|title$="tait"], p[|title$="tait"] { background-color : lime }
*|*[|title$="tait"], *|*[html|title$="tait"] { background-color : red }
</style>
	<div class="testText">
<p title="si on chantait" background-color="lime" display="block" margin-bottom="1.000000em">This paragraph should have a green background.</p>
<q xmlns="http://www.example.org/a" xmlns:a="http://www.example.org/a" a:title="si nous chantions" display="block" margin-bottom="1.000000em">This paragraph should be unstyled.</q>
<r xmlns="http://www.example.org/a" xmlns:a="http://www.example.org/a" a:title="si on chantait" background-color="lime" display="block" margin-bottom="1.000000em">This paragraph should have a green background.</r>
<s xmlns="http://www.example.org/b" xmlns:b="http://www.example.org/b" b:title="si on chantait" display="block" margin-bottom="1.000000em">This paragraph should be unstyled.</s>
</div>
</test>